dir /var/cache/fscache
tag mycache
brun {{ sambamount_cache_limit_run }}
bcull {{ sambamount_cache_limit_cull }}
bstop {{ sambamount_cache_limit_stop }}
frun {{ sambamount_cache_limit_run }}
fcull {{ sambamount_cache_limit_cull }}
fstop {{ sambamount_cache_limit_stop }}

# Assuming you're using SELinux with the default security policy included in
# this package
secctx system_u:system_r:cachefiles_kernel_t:s0
